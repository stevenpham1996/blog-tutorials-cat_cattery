<?xml version="1.0" encoding="utf-8"?>

<odoo>

  <!--tree -->
  <record model="ir.ui.view" id="breeder_breed_view_tree">
      <field name="name">breeder.breed.tree</field>
      <field name="model">cat_breeder.breed</field>
      <field name="arch" type="xml">
          <tree>
              <field name="name"/>
          </tree>
    </field>
  </record>

<!-- Form -->
  <record model="ir.ui.view" id="breeder_breed_view_form">
      <field name="name">breeder.breed.form</field>
      <field name="model">cat_breeder.breed</field>
      <field name="arch" type="xml">
          <form string="Breed">
              <field name="name"/>
          </form>
      </field>
  </record>

  <!-- Search -->
  <record id="view_search_breed" model="ir.ui.view">
    <field name="name">breeder.breed.search</field>
    <field name="model">cat_breeder.breed</field>
    <field name="arch" type="xml">
      <search>
        <field name="name"/>
      </search>
    </field>
  </record>

  <!-- Action -->
  <record id="action_breeder_breed" model="ir.actions.act_window">
      <field name="name">Breeds</field>
      <field name="res_model">cat_breeder.breed</field>
      <field name="view_mode">tree,form</field>
  </record>
</odoo>
